var currentAdd = window.location.href;



var currentLoginUrl = "http://www.yuhang.gov.cn/yuhangyhzx"; //需要更改的代码中的域名地址和应用名称



function jsload(data) {



    // 是否是登录的



    if (data.islogin) {



        var userSrc = data.gotourl;



        var loginoutScr = currentLoginUrl + "/member/login/logout.do?src=" + currentAdd;



        $("#login").empty();



        $("#login").html('<p style="cursor:default;">用户中心</p><div class="user-center"><a href="' + userSrc + '" target="_blank">查看详情</a><a class="login-out" href="' + loginoutScr + '">退出</p></div>');



    } else {



        var grSrc = data.grloginurl + "&goto=" + currentAdd;



        var frSrc = data.frloginurl + "?goto=" + currentAdd;



        var loginHtml = "用户中心<div class=\"loginItems\"><a class=\"gr_login\" href=\"" + grSrc + "\">个人用户</a><a class=\"fr_login\" href=\"" + frSrc + "\">法人用户</a></div>";



        $(".loginBtns").html(loginHtml);



    }



    $("#login>p,.user-center").hover(



        function () {



            $(".user-center").show();



        },



        function () {



            $(".user-center").hide();



        }



    );



};



document.writeln('<script type="text/javascript" src="' + currentLoginUrl + '/member/login/showUserResult.do">\<\/script>');







documen.writeln("  <div class=\'header-top-nav\'>");










